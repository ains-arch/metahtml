          <script>   
              window.getFormEvent = function() {
                MktoForms2.onFormRender(function() {
                  var eventID = jQuery('#event-detail-id').val();
                  var eventName = jQuery('.event__headline-title').text();
                  jQuery('[name="tHFLecturesandSeminarsRegistrations"]').val(eventID);
                  jQuery('[name="tHFThisWeekatHeritageSubscriber"]').val('no');
                  jQuery('[name="tHFLecturesandSeminarsSubscriber"]').val('no');
                });

                MktoForms2.loadForm("//app-ab04.marketo.com", "824-MHT-304", 2025, function(form) {
                  form.onSuccess(function(values, followUpUrl) {
                    jQuery('.event__rsvp-modal').addClass('_form-success');
                    window.removeFormEvent();
                    return false;
                  });


                  form.onSubmit(function() {
                    var email = jQuery('.event__rsvp-modal').find('#Email');
                    var emailVal = jQuery('.event__rsvp-modal').find('#Email').val();
                    if(email.length > 1){                      
                      email.each(function(e){
                        var email_string = jQuery(this).val();                        
                        if(email_string !== ''){
                          emailVal = email_string;
                        }                        
                      });
                    }                                        
                    jQuery('.event__rsvp-success-email').html(emailVal);
                  });

                });
              }

              window.removeFormEvent = function() {
                jQuery('#mktoForm_2025').remove();
                jQuery('.rsvp-form-container').append('<form id="mktoForm_2025"></form>');
              }
            </script>
