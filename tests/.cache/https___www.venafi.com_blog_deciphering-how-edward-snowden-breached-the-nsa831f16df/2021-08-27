                <center><h3>Sign up for Venafi Cloud</h3></center><br>
                <p><center><font color="#000000" size="4">Venafi Cloud manages and protects certificates</font></center></p>
                <br><br>
                <!-- https://ui.venafi.cloud/v1/useraccounts-->
                <script src="//app-sj08.marketo.com/js/forms2/js/forms2.min.js"></script>
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
                <script>
                    jQuery(document).ready(function () {
                        jQuery('.form-api #loading').hide();
                        jQuery('.thank-you-msg').hide();
                        jQuery('.view-agreement .btn').click(function (e) {
                            e.preventDefault();
                            jQuery(".license-overlay #terms").css('height', '100%');
                            jQuery(".risk-assessment-overlay ").css("overflow","hidden");
                            jQuery('body').addClass('body-overlay');
                        });
                        jQuery('.license-overlay .closebtn').click(function (e) {
                            e.preventDefault();
                            jQuery(".license-overlay #terms").css('height', '0%');
                            jQuery(".risk-assessment-overlay ").css("overflow","scroll");
                            jQuery('body').removeClass('body-overlay');
                        });
                        jQuery('.license-overlay .btn').click(function () {
                            jQuery(".license-overlay #terms").css('height', '0%');
                            jQuery(".risk-assessment-overlay ").css("overflow","scroll");
                            jQuery('.form-api .warning-msg img:first-child').css('display', 'none');
                            jQuery('.form-api .warning-msg img:last-of-type').css('display', 'block');
                            jQuery('.form-api .we-submit').css('display', 'block');
                            jQuery('.form-api .view-agreement').css('display', 'none');
                            jQuery('body').removeClass('body-overlay');
                        });
                        jQuery('.show-pass').on('click', function () {
                            var passwordField = $('.form-api .pass');
                            var passwordFieldType = passwordField.attr('type');
                            if (passwordFieldType == 'password')
                            {
                                passwordField.attr('type', 'text');
                                jQuery(this).text('Hide');
                            } else
                            {
                                passwordField.attr('type', 'password');
                                jQuery(this).text('Show');
                            }
                        });
                        var form1 = jQuery('#we_form1'),
                            form2 = jQuery('.hidden-form');
                        jQuery('#clone').click(function () {
                            jQuery(':input[id]', form2).val(function () {
                                return jQuery(':input[id=' + this.name + ']', form1).val();
                            });
                        });
                        jQuery('#we_form1').submit(function (e) {
                            jQuery('.form-api #loading').show();
                            var recaptcha = jQuery("#we_form1 #g-recaptcha-response").val();
                            jQuery("#recaptcha").val(recaptcha);
                            var formData = jQuery("#we_form1").serializeArray(),
                                email = formData[0].value,
                                password = formData[1].value,
                                firstname = formData[2].value,
                                lastname = formData[3].value,
                                companyName = formData[4].value,
                                account = formData[7].value;
                            jQuery.ajax({
                                type: 'POST',
                                url: 'https://api.venafi.cloud/v1/useraccounts',
                                contentType: "application/json",
                                dataType: 'json',
                                data: JSON.stringify({
                                    "userAccountType": account,
                                    "username": email,
                                    "password": password,
                                    "firstname": firstname,
                                    "lastname": lastname,
                                    "companyName": companyName,
                                    "grecaptchaResponse": recaptcha
                                }),
                                success: function (data) {
                                    jQuery('.risk-assessment-overlay .hidden-form .mktoButton').trigger('click');
                                    jQuery('.form-api').hide();
                                    jQuery('.thank-you-msg').show();
                                },
                            }).fail(function (data) {
                                var json_data = jQuery.parseJSON(data.responseText);
                                var usererror = json_data.errors[0].code,
                                    usermsg = json_data.errors[0].message;
                                if (usererror == 10103) {
                                    jQuery('#error').html(usermsg);
                                    jQuery('.form-api #loading').hide();
                                    grecaptcha.reset();
                                }
                                if (usererror == 10104) {
                                    jQuery('#error').html(usermsg);
                                    jQuery('.form-api #loading').hide();
                                    grecaptcha.reset();
                                }
                                if (usererror == 10106) {
                                    jQuery('#error').html(usermsg);
                                    jQuery('.form-api #loading').hide();
                                    grecaptcha.reset();
                                }
                                if (usererror == 10128) {
                                    jQuery('#error').html(usermsg);
                                    jQuery('.form-api #loading').hide();
                                    grecaptcha.reset();
                                }
                            });
                            e.preventDefault();
                            return false;
                        })
                    });
                    jQuery(window).load(function () {
                        jQuery('#Country').val($("#Country option:eq(1)").val());
                        jQuery('#Country').css('opacity', '1');
                    });
                </script>
                <script>function followUpInit1() {
                        var a = "https://www.venafi.com/node/981";
                        "undefined" != typeof MktoForms2 && MktoForms2.whenReady(function (b) {
                            b.onSuccess(function () {
                                console.log('test');
                            })
                        })
                    }
                    function addEvent(a, b, c) {
                        b.addEventListener ? b.addEventListener(a, c, !1) : b.attachEvent ? b.attachEvent("on" + a, c) : b[a] = c
                    }
                    addEvent("load", window, function () {
                        followUpInit1()
                    });</script>
