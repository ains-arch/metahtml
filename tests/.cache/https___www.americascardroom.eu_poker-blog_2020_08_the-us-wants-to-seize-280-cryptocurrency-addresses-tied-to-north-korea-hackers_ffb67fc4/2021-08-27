			function redirect(){
				var OSName="Desconocido";

				if (navigator.appVersion.indexOf("Win")!=-1){

					window.location = "https://play.americascardroom.eu/installer/SetupAmericasCardroom.exe";
					setTimeout(function() {
						window.location = "https://www.americascardroom.eu/downloading/";
					}, 1000);
				}
				if (navigator.appVersion.indexOf("Mac")!=-1){
					window.location = "https://play.americascardroom.eu/installer/AmericasCardroom.dmg";
