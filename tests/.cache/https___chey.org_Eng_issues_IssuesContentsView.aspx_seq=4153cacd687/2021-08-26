             function fnMainSearchKey() {
                 if (event.keyCode === 13) {
                   fnMainSearch()
                 };
            }

            function fnMainSearch() {
                if( $("#searchWord").val()=='') 
                {
                    alert("Please enter what you want to search");
                    $("#searchWord").focus();
                    return false;
                }
                location.href="/Eng/Etc/SearchList.aspx?h=N&search_txt="+ $("#searchWord").val();
               
            }

