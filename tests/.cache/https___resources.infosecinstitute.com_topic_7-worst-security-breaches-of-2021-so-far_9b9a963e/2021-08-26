	// Parse URL
	function getParameterByName(name) {
		name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
		var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
			results = regex.exec(location.search);
		return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}

	// Objects
	var dataLoad = ({
		page_type: 'Form',
		channel: 'wordpress',
		customerActivityDate: '2021-08-27',
	});

	var dataDigiohSuccess = ({ /* 4229 */
		page_type: 'Form',
		tealium_event: 'digioh_form_success',
	});

	/*  UTMs
	var utm_campaign = getParameterByName('utm_campaign');
	if (utm_campaign != '') {
		dataLoad.utm_campaign = utm_campaign;
	}

	var utm_content = getParameterByName('utm_content');
	if (utm_content != '') {
		dataLoad.utm_content = utm_content;
	}

	var utm_medium = getParameterByName('utm_medium');
	if (utm_medium != '') {
		dataLoad.utm_medium = utm_medium;
	}

	var utm_source = getParameterByName('utm_source');
	if (utm_source != '') {
		dataLoad.utm_source = utm_source;
	}

	var utm_term = getParameterByName('utm_term');
	if (utm_term != '') {
		dataLoad.utm_term = utm_term;
	} */

	// Assemble Tealium Data
	var dataDigiohEvent = ({
		tealium_event: 'digioh_form_load',
		page_type: 'Form',
		channel: 'wordpress2',
		customerActivityDate: '2021-08-27',
	});

	// Tealium Tag Reader
	let utagEmail;
	let utagSessionID;
	let utagTealiumID;

	function tealiumTagReader() {
		if (typeof utag !== 'undefined') {
			//console.log("UTAG FOUND");
			let iqQualifiedLead = utag.data['va.flags.40028'];
			utagEmail = utag.data['va.badges.11714'];
			utagSessionID = utag.data['ut.session_id'];
			utagTealiumID = utag.data['tealium_visitor_id'];

			if( iqQualifiedLead == true ){
				iqQualifiedLead = 'true';
				console.log("iqQualifiedLead = " + iqQualifiedLead);
				parent.window.iqQualifiedLead = iqQualifiedLead;
			} else {
				iqQualifiedLead = 'false';
				console.log("iqQualifiedLead = " + iqQualifiedLead);
				parent.window.iqQualifiedLead = iqQualifiedLead;
			}

			if( utagEmail == true ){
				utagEmail = 'true';
				console.log("utagEmail = " + utagEmail);
				parent.window.utagEmail = utagEmail;
				window.utagEmail = utagEmail;
			} else {
				utagEmail = 'false';
				console.log("utagEmail = " + utagEmail);
				parent.window.utagEmail = utagEmail;
				window.utagEmail = utagEmail;
			}
			if (utagTealiumID) {
				dataLoad.utagTealiumID = utagTealiumID;
				dataDigiohEvent.utagTealiumID = utagTealiumID;
				dataDigiohSuccess.utagTealiumID = utagTealiumID;
				//console.log("utagTealiumID = " + utagTealiumID);
			}
			if (utagSessionID) {
				dataLoad.utagSessionID = utagSessionID;
				dataDigiohEvent.utagSessionID = utagSessionID;
				dataDigiohSuccess.utagSessionID = utagSessionID;
				//console.log("utagSessionID = " + utagSessionID);
			}
		} else {
			console.log("NO UTAG FOUND");
		}
	}

	setTimeout(tealiumTagReader, 2000);

	// Send Message
	window.addEventListener('message', function(message) {
		//const packet = message.data.packet;
		const channel = message.data.channel;
		const id = message.data.id;
		const utm_campaign = message.data.utm_campaign;
		const utm_content = message.data.utm_content;
		const utm_medium = message.data.utm_medium;
		const utm_source = message.data.utm_source;
		const utm_term = message.data.utm_term;
		const customerActivity = message.data.customerActivity;
		const customerActivityType = message.data.customerActivityType;
		const internalSFCampaign = message.data.internalSFCampaign;
		const formName = message.data.formName;

		if (channel === 'digioh-information') { // Parent Box
			const digiohCountry = message.data.country;
			const digiohBoxName = message.data.lightbox_name;

			dataDigiohEvent.digiohCountry = digiohCountry;
			dataDigiohSuccess.formName = digiohBoxName;
		}

		if (channel === 'digioh_lightbox_load') { // 3946
			console.log('Lightbox Load Message Received');
			var lightbox = document.getElementById('lightbox-iframe-' + id);

			// Assemble Tealium Data
			/* if (utm_campaign) {
				dataLoad.utm_campaign = utm_campaign;
			}
			if (utm_content) {
				dataLoad.utm_content = utm_content;
			}
			if (utm_medium) {
				dataLoad.utm_medium = utm_medium;
			}
			if (utm_source) {
				dataLoad.utm_source = utm_source;
			}
			if (utm_term) {
				dataLoad.utm_term = utm_term;
			} */
			if (customerActivity) {
				dataLoad.customerActivity = customerActivity;
				dataDigiohEvent.customerActivity = customerActivity;
			}
			if (customerActivityType) {
				dataLoad.customerActivityType = customerActivityType;
				dataDigiohEvent.customerActivityType = customerActivityType;
			}
			if (internalSFCampaign) {
				dataLoad.internalSFCampaign = internalSFCampaign;
				dataDigiohEvent.internalSFCampaign = internalSFCampaign;
			}
			if (formName) {
				dataLoad.formName = formName;
				dataDigiohEvent.formName = formName;
			}

			lightbox.contentWindow.postMessage(dataLoad, '*');
			console.log(dataLoad);

			// Send ID Values
			function sendID() {
				var lightbox = document.getElementById('lightbox-iframe-' + id);
				lightbox.contentWindow.postMessage(dataDigiohEvent, '*');
				console.log("dataDigiohEvent.visitorID = " + dataDigiohEvent.visitorID);
			}

			setTimeout(sendID, 2250);

			// Tealium
			utag.view(dataDigiohEvent);
		}

		if (channel === 'digioh-success') { /* 4229 */
			dataDigiohSuccess.customerEmail = message.data.customerEmail;
			utag.view(dataDigiohSuccess);
		}

	});

	// Categories
	let resCertification = '';
	parent.window.resCertification = resCertification;
	window.resCertification = resCertification;
	console.log("Certification (resCertification) = " + resCertification);

	let resTopic = 'News';
	parent.window.resTopic = resTopic;
	window.resTopic = resTopic;
	console.log("Topic (resTopic) = " + resTopic);

	let articleURLPath = window.location.pathname;
	//console.log("Pathname (articleURLPath) = " + articleURLPath);

	let resCareer = articleURLPath.includes("career");
