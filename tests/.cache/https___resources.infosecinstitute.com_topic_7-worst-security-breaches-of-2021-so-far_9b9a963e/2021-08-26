	window.resCareer = resCareer;
	//console.log("Is 'career' in URL: = " + resCareer);

	$('head').append('<link rel="stylesheet" type="text/css" href="https://infosec-institute.s3-us-west-2.amazonaws.com/miscellaneous/resForms.css">');
	// Form Variables
	let modalWrapper = document.getElementById("modal-wrapper");
	let modal = document.getElementById("modal");
	let formPlacement = document.getElementById("form-wrapper");
	let formProcessing = document.getElementById("form-processing");
	let formController = document.getElementById('form');
	let formHeadline = document.getElementById("form-headline");
	let formSubheadline = document.getElementById("form-subheadline");
	let successButton = document.getElementById('bootcamp-link');
	let successSecondary = document.getElementById("secondary-success");

	// Form Data
	var myData = ({
		page_type: 'Form',
		page_name: '7 worst security breaches of 2021 (so far)',
		channel: 'resources',
		customerActivityDate: '2021-08-27',
