	// Modal
	function modalOpen2() {
		modalWrapper.style.opacity = "1";
	}

	function modalOpen() {
		modalWrapper.style.display = "flex";
		utag.view(dataDigiohEvent);
		setTimeout(modalOpen2, 500);
	}

	function closeModal2() {
		modalWrapper.style.display = "none";
	}

	function closeModal() {
		modalWrapper.style.opacity = "0";
		setTimeout(closeModal2, 500);
	}

	// Form States
	function startProcessing2() {
		formProcessing.style.opacity = "1";
		var primaryWrapper = document.getElementById("lightbox-inline-form-ccf21f05-4714-4671-b1a5-128ffc7d843e");
		if (primaryWrapper) {
			redirect();
		}
	}
	function startProcessing1() {
		formProcessing.style.display = "flex";
		setTimeout(startProcessing2, 40);
	}

	function stopProcessing2() {
		formProcessing.style.display = "none";
	}
	function stopProcessing1() {
		formProcessing.style.opacity = "0";
		setTimeout(stopProcessing2, 200);
	}

	function error2() {
		formController.style.left = "0";
	}
	function error1() {
		stopProcessing1();
		setTimeout(error2, 40);
	}

	function success2() {
		formController.style.left = "-200%";
	}
	function success1() {
		stopProcessing1();
		setTimeout(success2, 40);
	}

	// Message Reading
	window.addEventListener('message', function(message) {
		const channel = message.data.channel;
		// Form Loading
		if (channel === 'digioh_inline_load') { // 4257
			var iframePrimary = document.getElementById("lightbox-iframe-ccf21f05-4714-4671-b1a5-128ffc7d843e");
			if (iframePrimary) {
				iframePrimary.contentWindow.postMessage(myData, '*');
			}
			var iframeSecondary = document.getElementById("lightbox-iframe-897765c4-a11d-4b95-ae24-3ca928183a88");
			if (iframeSecondary) {
				iframeSecondary.contentWindow.postMessage(myData, '*');
			}
			setTimeout(stopProcessing1, 1000);
		}
		// Form Processing
		if (channel === 'formProcessing') { // 3696
			setTimeout(startProcessing1, 0);
			console.log("Processing...");
		}
		// Form Submit
		if (channel === 'formSubmit') { // 3696
			console.log("Digioh data recieved: " + message.data.customerFirstName + " " + message.data.customerLastName);
			myData.tealium_event = message.data.tealium_event;
			myData.customerFirstName = message.data.customerFirstName;
			myData.customerLastName = message.data.customerLastName;
			myData.companyName = message.data.companyName;
			myData.customerEmail = message.data.customerEmail;
			myData.customerPhone = message.data.customerPhone;
			myData.companySize = message.data.companySize;
			myData.trainingFundingMethod = message.data.trainingFundingMethod;
			myData.trainingBudget = message.data.trainingBudget;
			myData.studentFinancing = message.data.studentFinancing;
			myData.digiohCRMID = message.data.digiohCRMID;
			var generalForm = document.getElementById("lightbox-inline-form-897765c4-a11d-4b95-ae24-3ca928183a88");
			if (generalForm) {
				myData.customerCertificationInterest = message.data.customerCertificationInterest;
			}
			// Backup
			let postData = {action: "isi_backup_data",payload: myData,security: ajaxNonce};
			try {
				var request = jQuery.ajax({
					type: "POST",
					url: ajax_data.ajax_url, // Passed in via plugin
					action: "isi_backup_data",
					dataType: "json",
					data: postData,
				});
				request.done(function (data) {
					success1();
					console.log("Data returned: ", data);
				});
				request.fail(function (jqXHR, textStatus) {
					error1();
					console.log("Request failed: " + textStatus, jqXHR);
				});
			} catch(e) {
				error1();
				console.log("Error backing up data:",e);
			}
			// Tealium
			utag.view(myData);
			console.log("Data sent to Tealium");
			//success1();
		}
	});

</script>
