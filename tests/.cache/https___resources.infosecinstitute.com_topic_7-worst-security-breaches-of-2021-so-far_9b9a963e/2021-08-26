	// Redirect
	function redirect() {
		var counterWrapper = document.getElementById("countdown");
		var counterStart = 5;
		var download = document.getElementById("bootcamp-link");
		var downloadTimer = setInterval(function(){
			if(counterStart <= 0){
				clearInterval(downloadTimer);
				counterWrapper.innerHTML = "0";
				download.click();
			} else {
				counterWrapper.innerHTML = counterStart;
			}
			counterStart -= 1;
		}, 1000);
	}

	// Display Form
	function displayFormPrimary(name) {
		let digiohBoxPrimary = document.createElement('div');
		digiohBoxPrimary.setAttribute("id", "lightbox-inline-form-ccf21f05-4714-4671-b1a5-128ffc7d843e");
		digiohBoxPrimary.className = "form";
		formPlacement.appendChild(digiohBoxPrimary);
		formSubheadline.innerHTML = name;
	}

	function displayFormSecondary() {
		console.log("Secondary form");
		let digiohBoxSecondary = document.createElement('div');
		digiohBoxSecondary.setAttribute("id", "lightbox-inline-form-897765c4-a11d-4b95-ae24-3ca928183a88");
		digiohBoxSecondary.className = "form-secondary";
		modalWrapper.classList.add("form-outer-wrapper-secondary");
		formPlacement.appendChild(digiohBoxSecondary);
		formHeadline.innerHTML = "Get boot camp pricing";
		formSubheadline.style.display = "none";
	}

	// Processing
	function primaryProcessor(name, slug) {
		displayFormPrimary(name);
		myData.customerCertificationInterest = slug;
		myData.utm_term = name;
		myData.formName = 'Resources Boot Camp: ' + name;
		var successLink = 'https://www.infosecinstitute.com/courses/instant-pricing/?utm_course=' + slug + '&status=unlocked';
		successButton.href=successLink;
		modal.classList.add("form-outer-wrapper-primary");
	}

	function secondaryProcessor() {
		displayFormSecondary();
		myData.utm_term = 'Resources Boot Camp: General';
		myData.formName = 'Resources Boot Camp: General';
		successSecondary.innerHTML = "An Infosec representative will be in touch soon.";
	}

	// Ads
	let ad = document.getElementsByClassName("form-ad");
	let adHeadline = document.getElementsByClassName("ad-headline");
	let adCopy = document.getElementsByClassName("ad-copy");
	let adButton = document.getElementsByClassName("ad-cta");

	function adContents() {
		for(var i = 0; i < adHeadline.length; i++){
			adHeadline[i].innerHTML = "Get live, expert " + resCertification + "  instruction from anywhere";
		}
		for(var i = 0; i < adCopy.length; i++){
			adCopy[i].innerHTML = "Enroll in an upcoming live online boot camp and earn your " + resCertification + ", guaranteed.";
		}
	}

	// Match Certification to ISI Slug
	if (resCertification === 'A+') {
