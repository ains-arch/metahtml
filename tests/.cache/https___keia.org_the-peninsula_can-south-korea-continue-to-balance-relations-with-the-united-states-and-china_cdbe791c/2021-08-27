<script type="text/javascript">

    $(window).on('load', function () {
        // if the user clicks on a state in the data viz map
        // the button will have it's href value updated
        $("#usaMap path").click(function () {
            let state = $(this).attr('ID'),
                alDists = ['AK', 'DE', 'MN', 'ND', 'SD', 'VT', 'WY'],
                dist = alDists.includes(state) ? 'AL' : '01',
                value = `${state}-${dist}`,
                btn = $("#districtDataButton"),
                href = btn.attr("href").split('?')[0] + "?dist=" + value;

            btn.attr('href', href);

        });
    });

</script>
</div>
            </div>
        </div>
