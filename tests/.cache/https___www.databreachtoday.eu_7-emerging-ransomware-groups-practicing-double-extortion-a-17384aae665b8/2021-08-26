<script>
    $('#modalAssetEmail').on('show.bs.modal', function (event) {
        $('#asset-email-share-modal').validate_popover({
            popoverPosition: 'bottom',
            errorClass: "has-error",
            ignoreTitle:true,
            rules: {
                "email_from":{required:true,email:true},
                "email_to[]":{required:true,email:true}
            },
            submitHandler: function() {
                sendAssetEmail('asset-email-share-modal');
            }
        });
    });

    $('#modalAssetEmail').on('hide.bs.modal', function () {
        if($('#asset-email-share-modal').length > 0)
        {
            $.validator.hide_validate_popover($("#modalAssetEmail input"));
            $('#modalAssetEmail input').removeClass("has-error");
        }
    });
</script>    

