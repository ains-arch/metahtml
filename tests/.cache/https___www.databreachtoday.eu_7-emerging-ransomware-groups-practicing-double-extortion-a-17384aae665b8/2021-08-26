<script>
   /*
    *
    *10
    */
    $('#modalRequestToShare').on('show.bs.modal', function (event) {
          $('#request-share-modal').validate_popover({popoverPosition: 'bottom', errorClass:"has-error", ignoreTitle:true,
                rules:{ "first_name":{required:true},
                        "name":{required:true},
                        "email":{required:true,email:true},
                        "country":{required:true},
                        "phone":{required:true},
                        "company":{required:true}
                      },
                submitHandler: function() {
                    submitFormPopup("getPermission","request-share-modal");
                }
          });
    });
    $('#modalRequestToShare').on('hide.bs.modal', function () {
          $.validator.hide_validate_popover($("#request-share-modal input,#request-share-modal select"));
          $('#modalRequestToShare input,#modalRequestToShare select').removeClass("has-error");
    });
