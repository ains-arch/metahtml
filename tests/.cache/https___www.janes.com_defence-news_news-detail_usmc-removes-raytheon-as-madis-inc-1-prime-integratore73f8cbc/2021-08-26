    function replaceIndexCatalogueForTags() {
        var currentIndexCatalogue = "all";
        if (document.querySelectorAll('[data-sf-role="indexCatalogue"]').length > 0) {
            currentIndexCatalogue = document.querySelectorAll('[data-sf-role="indexCatalogue"]')[0].getAttribute("value");
