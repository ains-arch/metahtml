	<script type="text/javascript">
	(function($, viewport) {
		var promo = {
			active: false,
			available: false
		};

		// Execute code each time window size changes
		$(window).resize(
			viewport.changed(function() {
				var hasSubMenu = $('#nav-resources ul').hasClass('sub-menu');
				if( viewport.is('<=sm') ) {
					removeStickyClasses();
					resetNavTop();

					if(hasSubMenu){
						$('#nav-resources ul').removeClass('sub-menu');
					}
				} else {
					if(!hasSubMenu){
						$('#nav-resources ul').addClass('sub-menu');
						$('nav.main').addClass('sticky');
					}
				}
			})
		);

		function resetNavTop(){
			$('nav.main').css({
				top: "0px"
			});
		}

		function removeStickyClasses(){
			if($('nav.main').hasClass('sticky')){
				$('nav.main').removeClass('sticky');
			}
			if($('nav.main').hasClass('sticky-top')){
				$('nav.main').removeClass('sticky-top');
			}
		}

		function addStickyClasses(){
			if(!$('nav.main').hasClass('sticky')){
				$('nav.main').addClass('sticky');
			}
		}

		function addSubmenuDropdownAction(){
			hasSubMenu = $('#nav-resources ul').hasClass('sub-menu');
			if(!hasSubMenu){
				$('#nav-resources ul').addClass('sub-menu');
			}
		}

		function removeSubmenuDropdownAction(){
			hasSubMenu = $('#nav-resources ul').hasClass('sub-menu');
			if(hasSubMenu){
				$('#nav-resources ul').removeClass('sub-menu');
			}
		}

		function checkWindowSize(viewport){
			if( viewport.is('<=sm') ) {
				removeSubmenuDropdownAction();
				removeStickyClasses();
				resetNavTop();
			}

			// Executes in LG breakpoints
			if( viewport.is('>=md') ) {
				addSubmenuDropdownAction();
				addStickyClasses();
			}
			if ( !$("nav.main").hasClass('js') ) {
				$("nav.main").addClass("js");
			}
		}

		function swapStickies(){
			if( $('nav.main').hasClass('sticky-top') ) {
				$('nav.main').removeClass('sticky-top').addClass('sticky');
			} else if( $('nav.main').hasClass('sticky') ) {
				$('nav.main').removeClass('sticky').addClass('sticky-top');
			}
		}

		function handleScroll(){
			if( viewport.is('>=md') ){
				addStickyClasses();
				var header = $('header').height();
				var navMain = $('nav.main');
				var navMainTop = navMain.offset().top;
				if( $(window).scrollTop() >= (navMainTop) ){
					swapStickies();
					$('nav.main').css({
						top: ($(window).scrollTop() - (header+3)) + "px"
					});
				} else {
					swapStickies();
					if($(window).scrollTop() <= header){
						resetNavTop();
					} else {
						$('nav.main').css({
							top: ($(window).scrollTop() - (header+3)) + "px"
						});
					}
				}
			}
		}
	})(jQuery, ResponsiveBootstrapToolkit);
	</script>
</div>
</body>
</html>
