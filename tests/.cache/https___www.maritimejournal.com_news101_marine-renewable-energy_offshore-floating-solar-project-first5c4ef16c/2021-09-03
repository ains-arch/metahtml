<script type="text/javascript">
    window.mercatorCaptcha = {};
    
    mercatorCaptcha.captchaValidated = false;
    mercatorCaptcha.captchaToken = '';
    
    mercatorCaptcha.onExpiredCaptcha = function () {
        mercatorCaptcha.captchaToken = '';
    };
    
    mercatorCaptcha.submitListener = function(callback) {
        var oReq = new XMLHttpRequest();
        oReq.overrideMimeType("application/json");
        oReq.addEventListener("load", function() {
            var response = JSON.parse(this.responseText);
            if (response.success) {
                var newsletter = document.getElementById('marketo-newsletter');
                newsletter.setAttribute('action', response.action);
                console.log('6');
                mercatorCaptcha.captchaValidated = true;
                callback();
            } else {
                mercatorCaptcha.captchaValidated = false;
            }
        });
        oReq.open("POST", document.getElementById('captcha-verificator').value + '?token=' + mercatorCaptcha.captchaToken);
        oReq.send();
    }
    
    
    mercatorCaptcha.onValidatedCaptcha = function (token) {
        mercatorCaptcha.captchaToken = token;
    };
    
    window.onLoadCaptcha = function () {
      grecaptcha.render('google_captcha', {
        'sitekey': '6LfYWU8UAAAAACyC_pr0SqpI2UEngQF-w6YwO0bM',
        'callback': mercatorCaptcha.onValidatedCaptcha,
        'expired-callback': mercatorCaptcha.onExpiredCaptcha
      });
    };
</script> <div class="mktoContainer">
