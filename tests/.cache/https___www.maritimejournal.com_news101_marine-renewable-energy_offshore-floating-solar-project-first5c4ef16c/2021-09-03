<script>

(function($){

    // Handle multiple Marketo forms in the same page.
    $.each($('.mktoContainer'), function(index, mktoContainer) {

        var $fallbackForm = $(mktoContainer).find('.fallbackForm');
        var formid = $fallbackForm.find('input[name="formid"]').val();
        var accountID = $fallbackForm.find('input[name="munchkinId"]').val();

        // Add container for feedback messages.
        $(mktoContainer).append('<div class="mktoFeedback"></div>');
        // Add hidden form tag required for Marketo Forms 2 API to post in background.
        $(mktoContainer).append('<form id="mktoForm_' + formid + '" style="display:none;"></form>');

        // Override what happens when the submit button is clicked on the fallback form.
        $fallbackForm.submit(function(e) {

            // Stop it from doing anything it might normally do.
            e.preventDefault();
            var self = this;
            
            window.mercatorCaptcha.submitListener(function() {
                var actionURL = $fallbackForm.attr('action');
                var pathArray = actionURL.split('/');
                var mktoHost = 'https://' + pathArray[2];
        
                $(self).hide();
                updateFeedback(self, 'waiting');
                packageAndSend(self, mktoHost, accountID, formid);
            });
        }); // End submit.

    }); // End each fallback form.
    
    // Customise and update submission feedback here.
    function updateFeedback(form, msgType) {

        var msg = '';

        switch (msgType) {
            case 'waiting':
                msg = 'Please wait, your form submission is being processed.';
                $(form).parent().find('.mktoFeedback').html(msg);
                break;
            case 'error':
                msg = 'Sorry, there was a problem submitting the form.';
                $(form).parent().find('.mktoFeedback').html(msg);
                break;
            case 'success':
                if (marketoThankyouUrl) {
                    $.ajax({
                        url: marketoThankyouUrl,
                        dataType: 'html',
                        cache: true,
                        success: function(data) {
                            msg = data;
                            if (!msg) {
                                msg = 'Thank you for submitting the form.';
                            }
                            $(form).parent().find('.mktoFeedback').html(msg);
                        },
                        error: function() {
                            msg = 'Thank you for submitting the form.';
                            $(form).parent().find('.mktoFeedback').html(msg);
                        }
                    });
                } else {
                    msg = 'Thank you for submitting the form.';
                    $(form).parent().find('.mktoFeedback').html(msg);
                }
                
                break;
            case 'revalidate':
                msg = 'Thank you for submitting the form.';
                console.warn('Marketo form could not get all the fields. There might be differences between the form and the Marketo data.');
                $(form).parent().find('.mktoFeedback').html(msg);
                break;
            case 'notvalid':
                msg = 'Marketo expects a (required) field that has not been filled in. Check your Marketo form configuration to see whats expected and use global site JS to validate form fields.';
                $(form).parent().find('.mktoFeedback').html(msg);
                break;
            default:
                msg = 'No feedback type provided.';
                $(form).parent().find('.mktoFeedback').html(msg);
                break;
        }    

    } // End updateFeedback.

    /*
     Use Forms 2.0 API to:
     1. Dynamically load Marketo form into hidden container.
     2. Populate its fields using original fallback form.
     3. Post the form in the background.
     http://developers.marketo.com/documentation/websites/forms-2-0/
    */
    function packageAndSend(form, mktoHost, accountID, formid) {

        MktoForms2.loadForm(mktoHost, accountID, formid, function(mktoForm) {
            
            window.mktoFormRaw = mktoForm;
            console.log("mktoForm", mktoForm);

            var fields = {};

            $(form).find('input, select, textarea').each(function() {
                fields[$(this).attr('name')] = $(this).val();
            });

            mktoForm.addHiddenFields(fields);

            // Form validation should be done in global site JS.
            // But let's use Forms 2 API to at least provide some
            // sort of feedback if that fails.
            if (!mktoForm.validate()) {
                updateFeedback(form, 'revalidate');
            } else {
                window.mktoForm = mktoForm;

                mktoForm.submit();
    
                // TO DO need fix here after Hack day, onSuccess is no longer being called locally.
                updateFeedback(form, 'success');
    
                mktoForm.onSuccess(function() {
                    updateFeedback(form, 'success');
                    // Take the user to a different page on successful submit, ignoring the form's configured followUpUrl
                    // location.href = "https://google.com/?q=marketo+forms+v2+examples";
                    // Return false to prevent the submission handler continuing with its own processing.
                    return false;
                });
            }
        });

    } // End packageAndSend.

}(jQuery));

$(function(){

    $.ajax({
        url: 'https://munchkin.marketo.net/munchkin.js',
        dataType: 'script',
        cache: true,
        success: function() {
            Munchkin.init('883-DWY-899');
        }
    });  

});

MktoForms2.whenReady(function (form) {
   //listen for the validate event
    form.onValidate(function() {
        
        if (mercatorCaptcha.captchaValidated && form.vals().honeypot == "") {
            form.submittable(true);
        } else {
            form.submittable(false);
        }
    });
});

</script> 
                  
                  
                  
                  
                  
