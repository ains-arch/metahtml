					var arr_length = arr_splits.length;

					if (arr_length > 1) {

						var dir_path = 'https://cisomag.eccouncil.org/wp-content/themes/Newsmag';
						var splited_css = '';

						for (var i = 0; i < arr_length; i++) {
							if (i > 0) {
