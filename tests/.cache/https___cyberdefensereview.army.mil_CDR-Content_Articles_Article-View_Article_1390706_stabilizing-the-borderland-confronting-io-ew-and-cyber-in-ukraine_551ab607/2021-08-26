        $("#comment_policy_content").hide();
        $("#comment_policy_toggle").click(function () {
            if ($('#comment_policy_content').is(':visible'))
                $("#comment_policy_icon").removeClass("up").addClass("down");
            else
                $("#comment_policy_icon").removeClass("down").addClass("up");
            $("#comment_policy_content").slideToggle('fast');
        });
    });
</script>

