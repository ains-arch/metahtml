/* Esto añade un velo oscuro a las fotos del headaer en el blog*/
.page-header-bg-image {
    -webkit-filter: brightness(60%);
    filter: brightness(60%);   
}

.nectar-recent-posts-single_featured.multiple_featured .controls li .title {
    line-height: 24px !important;
}

body[data-fancy-form-rcs="1"] .fancy-select-wrap label {
    display: none;
}

/* Esto quita el doble titulo en los widgets del lateral del blog*/
.screen-reader-text {
    display: none;
}

/* Esto permite que las imagenes en el footer aparezcan una al lado de la otra en vez de una encima de otra*/
.widget_media_image {
    float: left;
    padding-right: 10px;
}

/* Esto quita el padding en el footer para que los iconos no queden tan alejados*/
#footer-outer .row {
    padding: 55px 0 0 0;
}

/* Esto alinea los iconos con el texto en la barra morada de soporte*/
.row .col .iwithtext .iwt-icon img:not([srcset]) {
    padding-top: 4px;
    width: auto;
}

/* Esto acerca el icono al texto en la barra morada de soporte - por defecto estaba en 55px*/
.iwithtext .iwt-text {
    padding-left: 30px;
}

/* Esto elimina el paading entre el header y el contenido de los posts - por defecto estaba en 40px*/
body.single-post[data-bg-header="true"].ascend .container-wrap {
    padding-top: 0px!important;
}

/* Los 2 proximos comandos eliminan el paading entre el footer y el contenido de los posts*/
.row {
    padding-bottom: 0px;
}

.post .content-inner {
    padding-bottom: 0px;
}

/* Los 3 proximos comandos añaden un linea de color en la izquierda de los boxes de destacado ej. Orion*/
.linepurple {
	border-left: 15px solid #5e3079;
}
.linespadding {
	padding-right: 2%;
}

/* Esto cambia el color y el tamaño de la fuente en el modulo Post Grid */
.nectar-post-grid[data-text-color="dark"] .nectar-post-grid-item .content * {
    color: #5e3079 !important;
    font-size: 17px !important;
}

/* Los 2 siguientes cambian el tamaño y el padding de la caja que muestra cada post del modulo Post Grid*/
.nectar-post-grid[data-grid-item-height="30vh"] .nectar-post-grid-item {
    min-height: 120px;
}

.nectar-post-grid .nectar-post-grid-item .content {
    padding: 25px;
}
</style>
