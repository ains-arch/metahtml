<script>
var myurl = 'https://pacforum.org/publication/pacnet-17-japan-and-south-koreas-alternative-paths-in-the-indo-pacific';
jQuery( document ).ready(function() { 
jQuery("#text-4 .textwidget").find("button").parent("#text-4 .textwidget").addClass("download");
	jQuery('article.jupiterx-new-search').insertBefore('.jupiterx-main-content');
	
     $('#menu-navigation ul li a').each(function(){
      if($(this).attr('href')== myurl){
     $(this).parent().closest('ul').css('display','block');
     if($(this).parent().parent().closest('li').hasClass('jupiterx-icon-plus')){
     $(this).parent().parent().closest('li').toggleClass("jupiterx-icon-minus");
     }
      if($(this).parent().parent().closest('li').length>0){
   
   }else{
     
   }
   //console.log($(this).parent().parent().closest('li').length);
   $(this).next('ul').css('display','block');

    //$(this).next('li').toggleClass("jupiterx-icon-minus");
}
    });
     $('#menu-navigation li a').each(function(){
        if($(this).attr('href')== myurl){
       // $(this).closest('ul').css('display','block');
       $(this).next('ul').css('display','block');
       if($(this).parent().parent().closest('li').length>0){
        $('.card-title').show();
         
   
   }else{
    if($(this).closest("li").children("ul").length) {
        if($(this).closest('li').hasClass('jupiterx-icon-plus')){
     $(this).closest('li').toggleClass("jupiterx-icon-minus");
     }
       // the clicked on <li> has a <ul> as a direct child
   }else{
     $('.card-title').hide();
            $(this).hide();

   }
           
   }
       
   
}
    });
   $('ul.expandible').each(function(){
    var $ul = $(this),
        $lis = $ul.find('li:gt(4)'),
        isExpanded = $ul.hasClass('expanded');
    $lis[isExpanded ? 'show' : 'hide']();
    
    if($lis.length > 0){
        $ul
            .append($('<span class="showmore">' + (isExpanded ? 'Show Less' : 'Show More') + '</span>')
            .click(function(event){
                var isExpanded = $ul.hasClass('expanded');
                event.preventDefault();
                $(this).html(isExpanded ? 'Show More' : 'Show Less');
                $ul.toggleClass('expanded');
                $lis.toggle();
            }));
    }
});
  });
</script>

