        var estado = 1;
        function hiddeComentarios(){
            if(estado===1) {
                comentarios.style.display = 'block';
                btnComentarios.html="OCULTAR COMENTARIOS<span></span>";
                estado=2;

            }else{
                estado=1;
                btnComentarios.html="DEJAR COMENTARIOS<span></span>";
                comentarios.style.display = 'none';
            }

        }

    </script>

