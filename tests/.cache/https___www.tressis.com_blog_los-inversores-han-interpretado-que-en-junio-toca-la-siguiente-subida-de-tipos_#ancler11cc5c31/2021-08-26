        <script type="text/javascript">
            //<div class="expand" onclick="desplegar();">Blog</div>
            jQuery("#search-3").after('<div class="expand" onclick="desplegar();">BLOG</div>');

            //html('<div class="expand" onclick="desplegar();">Blog</div>');       
            function desplegar(){
                        if(jQuery('#categories-3').css('display') == 'none'){ 
                            jQuery('#categories-3').css('display','block');
                            jQuery('#nav_menu-4').css('display','block'); 
                        } else { 
                            jQuery('#categories-3').css('display','none');
                            jQuery('#nav_menu-4').css('display','none');   
                        }                              
                    }
            jQuery( ".cat-item,.menu-item" ).on( "click", function() {
                        jQuery('#categories-3').css('display','none');
