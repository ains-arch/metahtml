    .dropMenu .top2.conODS .inner-content > .container > .desktop_menu .yamm .navbar-collapse ul.navbar-nav { margin-top: 31px;}
}
@media only screen and (max-width:900px){
    /*-- MENU CON ODS --*/
    .conODS .yamm .navbar-nav > li > a { font-size: 15px;}
}
@media only screen and (max-width:850px){    
    /*-- MENU CON ODS --*/
    .conODS .yamm .navbar-nav > li > a { font-size: 14px;}
    .dropMenu .top2.conODS .inner-content > .container > .desktop_menu .yamm .navbar-collapse ul.navbar-nav > li > a { padding-left:12px !important;}
    .dropMenu .top2.conODS .inner-content > .container > .desktop_menu .yamm .navbar-collapse ul.navbar-nav > li > a::after, 
    .dropMenu .top2.conODS .inner-content > .container > .desktop_menu .yamm .navbar-collapse ul.navbar-nav > li:first-of-type > a::after { left: 12px;}
}

@media screen and (min-width: 800px) and (max-width: 1150px) {
    /*-- MENU CON ODS --*/
    .dropMenu .top2.conODS .inner-content > .container > .desktop_menu .yamm .navbar-collapse ul.navbar-nav {
        -webkit-transform: scale(.85);
        -moz-transform: scale(.85);
        -ms-transform: scale(.85);
        transform: scale(.85);
        -webkit-transform-origin: right center;
        -moz-transform-origin: right center;
