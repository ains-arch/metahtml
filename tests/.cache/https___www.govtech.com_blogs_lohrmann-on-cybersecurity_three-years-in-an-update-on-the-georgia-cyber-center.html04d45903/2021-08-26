        data-src="https://erepublic.brightspotcdn.com/dims4/default/033e951/2147483647/strip/true/crop/217x289+42+0/resize/330x440!/quality/90/?url=http%3A%2F%2Ferepublic-brightspot.s3.amazonaws.com%2F12%2Ffa%2F5fb76912cf1cfffef61cc8e90a5e%2Fsteve-nichols-georgia-cropped-2.jpg" data-lazy-load="true" src="data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHZlcnNpb249IjEuMSIgaGVpZ2h0PSI0NDBweCIgd2lkdGg9IjMzMHB4Ij48L3N2Zz4="
