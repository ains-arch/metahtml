<script type="text/javascript">
    function openInfo(event) {
        var $el=$('.info-button');
        var $pa=$("#adetail-slick-gallery");
        var $icon=$el.find('.fa');
        if($pa.hasClass('open')) {
            $pa.removeClass('open');
            $("#adetail-slick-gallery").animate({ height: $('.slick-current .aimage').first().height()+'px' },300);
            $icon.removeClass('fa-minus-circle');
            $icon.addClass('fa-plus-circle');
        } else {
            $pa.addClass('open');
            $("#adetail-slick-gallery").animate({ height: $('.slick-current').first().height()+'px' },300);
            $icon.addClass('fa-minus-circle');
            $icon.removeClass('fa-plus-circle');
        }
    }

    function GalleryInit() {
        jQuery("#adetail-slick-gallery").slick({
            fade: true,
            adaptiveHeight: true,
        });
        jQuery("#adetail-slick-gallery").on('afterChange, setPosition',
            function(event,currentSlide) {
                if ($("#adetail-slick-gallery").hasClass('open')) {
                    $("#adetail-slick-gallery").animate({height:$('.slick-current').first().height() + 'px'}, 300);
                } else {
                    $("#adetail-slick-gallery").animate({ height: $('.slick-current .aimage').first().height()+'px' },300);
                }
            });
    }
    $(window).load(function() {
        GalleryInit();
    });
</script>

<script type="text/javascript">

jQuery(document).ready(function() {
var prm = Sys.WebForms.PageRequestManager.getInstance();
if (prm){
if(typeof GalleryInit == 'function'){	prm.add_endRequest(GalleryInit);
} }
});

</script>

<script type="text/javascript">

jQuery(document).ready(function() {
var prm = Sys.WebForms.PageRequestManager.getInstance();
if (prm){
if(typeof SlideshowInit == 'function'){	prm.add_endRequest(SlideshowInit);
} }
});

</script>



