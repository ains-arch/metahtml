<script>
function submitForm(){
  getUrl = window.location;
  var baseUrl = getUrl.protocol + "//" + getUrl.host + "/" + getUrl.pathname.split('/')[1];
  var newSearchParam = document.getElementById('input_search').value;
  window.location = baseUrl + "/search?q=" + newSearchParam;
}
</script>


