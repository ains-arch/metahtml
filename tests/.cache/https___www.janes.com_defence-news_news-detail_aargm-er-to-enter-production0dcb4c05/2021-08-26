<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
    document.addEventListener("DOMContentLoaded", function (event) {
        replaceIndexCatalogueForTags();
        addWordModeToTagsSearchItems();
    });

    function replaceIndexCatalogueForTags() {
        var currentIndexCatalogue = "all";
        if (document.querySelectorAll('[data-sf-role="indexCatalogue"]').length > 0) {
            currentIndexCatalogue = document.querySelectorAll('[data-sf-role="indexCatalogue"]')[0].getAttribute("value");
        }

        var tagItems = document.getElementsByClassName("tag-srch-itm");
        for (var i = 0; i < tagItems.length; i++) {
            var newHref = tagItems[i].getAttribute("href").replace("[index-catalogue]", currentIndexCatalogue);
            tagItems[i].setAttribute("href", newHref);
        }
    }
    function addWordModeToTagsSearchItems(){
        if (document.querySelectorAll('[data-sf-role="wordsMode"]').length > 0) {
            var tagItems = document.getElementsByClassName("tag-srch-itm");
            for (var i = 0; i < tagItems.length; i++) {
                var newHref = tagItems[i].getAttribute("href") + "&wordMode=" + document.querySelectorAll('[data-sf-role="wordsMode"]')[0].getAttribute("value");
                tagItems[i].setAttribute("href", newHref);
            }
        }
    }

